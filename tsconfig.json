{
	"compilerOptions": {
		"target": "ES2017",
		"outDir": "./dist/",
		"strictNullChecks": true,
		"moduleResolution": "node",
		"allowJs": true,
		"strict": true,
		"esModuleInterop": true,
		"jsx": "react",
		"lib": [
			"ES2015",
			"DOM.Iterable",
			"ES2016.Array.Include",
			"ES2017.Object",
			"DOM"
		],
		"module": "CommonJS",
		"removeComments": true,
		"alwaysStrict": true,
		"allowUnreachableCode": false,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"sourceMap": true
	}
}